algoritmo "soma"

var
  numero_1:inteiro
  numero_2:inteiro
  soma:inteiro
  
inicio
  escreva("Digite um valor: ")
  leia(numero_1)
  
  escreval()
  
  escreva("Digite outro valor: ")
  leia(numero_2)
  
  escreval()
  
  soma<-numero_1+numero_2
  
  escreva("A soma entre ", numero_1, " e ", numero_2, " Ã© igual a ", soma)
  
fimalgoritmo
