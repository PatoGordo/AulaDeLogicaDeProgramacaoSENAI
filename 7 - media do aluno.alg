algoritmo "media_do_aluno"

var
  nota_1:real
  nota_2:real
  media:real
  
inicio
  escreva("Nota 1: ")
  leia(nota_1)
  
  escreval()
  
  escreva("Nota 2: ")
  leia(nota_2)
  
  escreval()
  
  media<-((nota_1 + nota_2) / 2)
  
  escreva("A média entre ", nota_1, " e ", nota_2, " é igual a ", media)
  
fimalgoritmo
